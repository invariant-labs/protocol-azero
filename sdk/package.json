{
  "name": "@invariant-labs/a0-sdk",
  "version": "0.2.17",
  "collaborators": [
    "Invariant Labs"
  ],
  "description": "",
  "type": "module",
  "main": "target/index.js",
  "keywords": [
    "Aleph Zero",
    "DEX",
    "SDK",
    "substrate",
    "ink"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/invariant-labs/protocol-a0",
    "directory": "sdk"
  },
  "scripts": {
    "a": "echo Running script a",
    "a:b": "echo Running script ab",
    "prebuild": "./prebuild.sh",
    "build": "npx tsc",
    "start:testnet": "npx tsc && node --experimental-wasm-modules target/src/index.js testnet",
    "start:local": "npx tsc && node --experimental-wasm-modules target/src/index.js local",
    "lint": "eslint . --ext .ts",
    "wasm:package": "./package.sh",
    "wasm:build": "cd src/wasm && wasm-pack build --features tsify/js",
    "node:start": "substrate-contracts-node purge-chain -y && substrate-contracts-node --rpc-port 9944 --dev",
    "node:stop": "ps aux | grep substrate-contracts-node | tr -s ' ' | cut -d ' ' -f 2 | head -n 1 | xargs -r kill -9",
    "contract:build": "cd .. && cargo contract build --release && cd src/token && cargo contract build --release && cd ../../sdk",
    "docs:copy": "cp ../README.md README.md",
    "build:copy-wasm": "cd target && mkdir wasm && cp -r ../src/wasm/pkg ./wasm/pkg",
    "test:all": "ts-mocha",
    "test:local": "npm run test:utils && npm run test:wazero && npm run test:psp22 && npm run test:protocol-fee  && npm run test:math && npm run test:invariant && npm run test:example && npm run test:events && npm run test:tx && npm run test:position && npm run test:get-position-with-associates && npm run test:get-positions && npm run test:get-all && npm run test:query-on-pair && npm run test:get-liquidity-ticks",
    "test:utils": "mocha ./tests/utils.test.ts -g utils",
    "test:wazero": "mocha ./tests/wrapped-azero.test.ts -g wrapped-azero",
    "test:psp22": "mocha ./tests/psp22.test.ts -g psp22",
    "test:protocol-fee": "mocha ./tests/protocol-fee.test.ts -g protocol-fee",
    "test:position": "mocha ./tests/position.test.ts -g position",
    "test:get-positions": "mocha ./tests/get-positions.test.ts -g get-positions",
    "test:get-position-with-associates": "mocha ./tests/get-position-with-associates.test.ts -g get-position-with-associates",
    "test:math": "mocha ./tests/math.test.ts -g get-liquidity-by-x && mocha ./tests/math.test.ts -g get-liquidity-by-y",
    "test:invariant": "mocha ./tests/invariant.test.ts -g invariant",
    "test:get-liquidity-ticks": "mocha ./tests/get-liquidity-ticks.test.ts -g get-liquidity-ticks",
    "test:get-tickmap": "mocha ./tests/get-tickmap.test.ts -g 'tickmap test'",
    "test:storage-limit": "mocha ./tests/storage-limit.test.ts -g storage-limit",
    "test:example": "mocha ./tests/example.test.ts -g sdk-guide-snippets",
    "test:events": "mocha ./tests/events.test.ts -g events",
    "test:crosses": "mocha ./tests/crosses-testnet.test.ts -g testnet-crosses-limitations",
    "test:tx": "mocha ./tests/tx.test.ts -g tx",
    "test:simulate-invariant-swap": "mocha ./tests/simulate-invariant-swap.test.ts -g simulateInvariantSwap",
    "test:query-on-pair": "mocha ./tests/query-on-pair.test.ts -g query-on-pair",
    "test:get-all": "mocha ./tests/get-all.test.ts -g get-all"
  },
  "dependencies": {
    "@polkadot/api": "^10.12.4",
    "@polkadot/api-contract": "^10.12.4",
    "@scio-labs/use-inkathon": "^0.6.3",
    "@invariant-labs/a0-sdk-wasm": "^0.1.23",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "chai": "^5.0.0",
    "eslint": "^8.56.0",
    "ts-mocha": "^10.0.0",
    "wasm-pack": "^0.12.1"
  }
}
